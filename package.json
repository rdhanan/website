{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rm -rf .next && concurrently -k -c 'bgMagenta,bgBlue' -n contentlayer,next 'contentlayer dev' 'next dev'",
    "build": "contentlayer build --clearCache && next build",
    "start": "next start",
    "lint": "eslint --cache --cache-strategy content --cache-location=node_modules/.cache/eslint/.eslint-cache src",
    "format": "prettier --check --cache --cache-strategy content .",
    "format:write": "prettier --write --cache --cache-strategy content .",
    "typecheck": "tsc --noEmit --incremental false",
    "commit": "git-cz",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@giscus/react": "^2.3.0",
    "@plaiceholder/next": "^3.0.0",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@upstash/redis": "^1.22.0",
    "contentlayer": "^0.3.4",
    "next": "^13.4.13",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "plaiceholder": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-use": "^17.4.0",
    "sharp": "^0.32.4",
    "swr": "^2.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@commitlint/types": "^17.4.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@tailwindcss/typography": "^0.5.9",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/fs-extra": "^11.0.1",
    "@types/mdx": "^2.0.6",
    "@types/node": "^20.4.9",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.13",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "feed": "^4.2.2",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-pretty-code": "^0.10.0",
    "rehype-shift-heading": "^1.0.2",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "shiki": "^0.14.3",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "//": [
    "feed depends on fs-extra",
    "plaiceholder depends on sharp"
  ]
}
